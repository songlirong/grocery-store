.car-guess {
  width: 100%;
  height: 120rpx;
}

.car-guess .guess-header {
  width: 120rpx;
  height: 100%;
  color: #212121;
  font-size: 26rpx;
  margin: 0 auto;
  line-height: 120rpx;
  text-align: center;
  padding-left: 50rpx;
  background: url('data:image/png;base64,') no-repeat 10rpx 41rpx / 38rpx 38rpx;
  position: relative;
}

.car-guess .line {
  width: 75rpx;
  height: 2rpx;
  background: #bdbdbd;
  position: absolute;
  top: 59rpx;
}

.car-guess .line-left {
  left: -80rpx;
}

.car-guess .line-right {
  right: -80rpx;
}

.car-guess .guess-content {
  width: 100%;
  height: auto;
}

.car-guess .guess-list {
  width: 100%;
  height: auto;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  background: #f5f5f5;
}

.car-guess .guess-item {
  width: 375rpx;
  height: 506rpx;
  background: #fff;
  border-bottom: 6px solid #f5f5f5;
  padding: 20rpx 30rpx 0 20rpx;
  box-sizing: border-box;
}

.car-guess .guess-item:nth-child(n) {
  border-right: 6px solid #f5f5f5;
}
.car-guess .guess-item:nth-child(2n) {
  border-right: none;
}

.car-guess .item-img {
  width: 100%;
  height: 240rpx;
  background: #09afff;
}

.car-guess .item-img image {
  width: 100%;
  height: 100%;
}

.car-guess .item-content {
  width: 100%;
  height: 100%;
}

.car-guess .item-names {
  width: 100%;
  margin-top: 20rpx;
  color: #4c4c4c;
  font-size: 28rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}

.car-guess .item-discount {
  width: 100%;
  height: 44rpx;
}

.car-guess .discount {
  width: 90rpx;
  height: 30rpx;
