.explore {
  width: 100%;
  height: auto;
  padding: 0 24rpx;
  box-sizing: border-box;
}

.explore-title {
  width: 100%;
  height: 77rpx;
  text-align: center;
  padding-top: 30rpx;
  font-size: 24rpx;
  font-weight: bold;
  color: #999;
  position: relative;
  box-sizing: border-box;
}

.explore-title .text {
  color: #76baf5;
  font-weight: bold;
}

.explore-title::before {
  content: '';
  display: block;
  width: 80rpx;
  height: 1rpx;
  background: #d4d4d4;
  position: absolute;
  top: 47rpx;
  left: 196rpx;
}

.explore-title::after {
  content: '';
  display: block;
  width: 80rpx;
  height: 1rpx;
  background: #ed6a35;
  position: absolute;
  top: 47rpx;
  right: 196rpx;
}

.explore-title::before {
  left: 149rpx;
}

.explore-title::after {
  background: #76baf5;
  right: 149rpx;
}

.information-h3 {
  height: 60rpx;
  font-size: 30rpx;
  color: #101010;
  line-height: 1.1;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.information-span {
  font-size: 24rpx;
  color: #acacac;
  line-height: 1.2;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.information-feature {
  width: 100%;
  height: 66rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  margin: 35rpx 0%;
}

.feature-identification {
  width: auto;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: flex-start;
}

.identification-h3 {
  font-size: 32rpx;
  color: #101010;
  line-height: 1.3;
}

.feature-vertical {
  width: 2rpx;
  height: 66rpx;
  margin: 0 35rpx;
  background: #e6e6e6;
}

.information-price {
  width: 100%;
  height: auto;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.information-price-num {
  font-size: 22rpx;
  color: #8b8b8b;
  line-height: 46rpx;
}

.explore .price-span {
  display: inline;
  font-size: 24rpx;
  color: #ff2828;
}

.explore .information-price-cart {
  width: 46rpx;
  height: 46rpx;
  border-radius: 50%;
  background: #09afff url(data:image/png;base64,) no-repeat center center / 28rpx 27rpx;
}

.explore {
  padding: 0;
}

.explore .explore-content {
