view {
  box-sizing: border-box;
}
.c-head {
  width: 100%;
  height: 142rpx;
  background: linear-gradient(to right, #0acaff, #0ab0ff);
  padding: 25rpx 25rpx 0;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 2rpx solid #f6f6f6;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 999;
  color: #fff;
}
.icon {
  margin-left: 14rpx;
  width: 44rpx;
  height: 30rpx;
  background: url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABIAAAASCAYAAABWzo5XAAAAdklEQVQ4T+2Syw2AIBAF53VgB9qBLVi6LdiBdmAHa0g8EH57gYuRG+ExmX0gOi114vCD/CbHdWRmCzA7DpekM85kRmY2ATuwVmAHsEm6m6Bw2IAVIeFOtaMCrAppghKzsM3GcUeLA68ZaSdpf+Oe3/965cSHjR61iyATGFRfHwAAAABJRU5ErkJggg==)no-repeat center center/100% auto;
}
.search {
  width: 44rpx;
  height: 44rpx;
  background: url(data:image/png;base64,)no-repeat center center /100% auto;
}
#shop {
  width: 35rpx;
  height: 35rpx;
  margin-left:25rpx;
  background: url(data:image/png;base64,)no-repeat center center /100% auto;
  position: relative;
}
.num-mark{
  width: 30rpx;
  height: 30rpx;
  border-radius: 50%;
  background: #FF4432;
  position: absolute;
  right:-15rpx;
  top:-10rpx;
  color: white;
  font-size: 22rpx;
  font-weight: bold;
  text-align: center;
  line-height: 30rpx; 
}
image {
  width: 100%;
  height: 100%;
}
.c-head-left {
  width: 44rpx;
  height: 44rpx;
  background: url(data:image/png;base64,)no-repeat center center /100% auto;
}
.c-head-right {
  width: 427rpx;
  height: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 36rpx;
}
.c-head-right .right1 {
  display: flex;
  align-items: center;
}
.c-body {
  width: 100%;
  height: 1334rpx;
  padding-top: 142rpx;
  overflow: hidden;
}
.c-body-left {
  height: 100%;
  width: 167rpx;
  background: #f7f7f7;
  float: left;
  overflow: scroll;
}
.c-body-scroll {
  height: auto;
}
.c-list {
  height: 95rpx;
  width: 100%;
  font-size: 24rpx;
  color: #3a3a3a;
  display: flex;
  justify-content: center;
  align-items: center;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.c-list.active {
  background: #fff;
  color: #2b2b2b;
  font-weight: bolder;
  position: relative;
}
.c-list.active::after{
  content:'';
  display: block;
  width: 9rpx;
  height: 54rpx;
  border-radius: 4rpx;
  background: #25a1fb;
  position: absolute;
  left:0;
  top:0;
  bottom:0;
  margin:auto;
}
.c-list:first-child::before {
  display: block;
  content: '荐';
  width: 27rpx;
  height: 27rpx;
  background: #e95b68;
  border-radius: 5rpx;
  margin-right: 4rpx;
  font-size: 18rpx;
  color: #fff;
  text-align: center;
  line-height: 27rpx;
}
.c-list:nth-child(2)::before {
  display: block;
  content: '荐';
  width: 27rpx;
  height: 27rpx;
  background: #e95b68;
  border-radius: 5rpx;
  margin-right: 4rpx;
  font-size: 18rpx;
  color: #fff;
  text-align: center;
  line-height: 27rpx;
}
.c-list:nth-child(3)::before {
  display: block;
  content: '新';
  width: 27rpx;
  height: 27rpx;
  background: #e95b68;
  border-radius: 5rpx;
  margin-right: 4rpx;
  font-size: 18rpx;
  color: #fff;
  text-align: center;
  line-height: 27rpx;
}
.c-body-right {
  float: right;
  width: calc(100% - 167rpx);
  height: 100%;
  padding-left: 20rpx;
  overflow: scroll;
}
.c-body-right.active{
  overflow: hidden;
}
.c-body-box {
  width: 538rpx;
  height: 234rpx;
  padding-right: 25rpx;
  display: flex;
  align-items: center;
}
.c-body-picture {
  width: 518rpx;
  height: 154rpx;
  border-radius: 20rpx;
  margin-left: 12rpx;
  background: #0ac7ff;
}
.scroll-title {
  width:563rpx;
  padding-right: 25rpx;
  height: 70rpx;
  display: flex;
  justify-content: space-between;
  position:relative;
  top: 0;
  background: #fff;
}
.scroll-title.active{
  position: fixed;
  top:142rpx;
  z-index: 888;
}
.xiala-box{
  width: 48rpx;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.scroll-xiala {
  width: 27rpx;
  height: 18rpx;
  transition: all 0.3s;
  background: url(data:image/png;base64,)no-repeat center center /100% auto;
}
.scroll-xiala.active {
  transform: rotate(180deg);
}
.shade {
  width: 750rpx;
  height: 1204rpx;
  position: fixed;
  right: 0;
  top: 130rpx;
  z-index: 1000;
  background: rgba(255, 255, 255,0.01);
}
.shade-bottom {
  width: 583rpx;
  height: 1334rpx;
  background: rgba(0, 0, 0, 0.3);
  position: absolute;
  right: 0;
  top: 70rpx;
  bottom:0;
  z-index:999;
}
.shade-box {
  width: 100%;
  height: 187rpx;
  background: #fff;
  position: relative;
  left:0;
  z-index: 99999;
}
.shade-list {
  width: 177rpx;
  height: 73rpx;
  background: #f5f5f5;
  font-size: 26rpx;
  color: #363636;
  text-align: center;
  line-height: 73rpx;
  float: left;
  margin: 10rpx 8rpx;
}
.shade-list.active {
  background: #eaf8ff;
  color: #0c99ff;
}
.scroll-box {
  width: 490rpx;
  height: 70rpx;
  background: linear-gradient(to right, white 95%, #d6d6d6 115%);
  overflow: hidden;
}
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
.scroll-box .title {
  width: auto;
  height: 100%;
  display: flex;
}
.title .c-nav {
  width: 160rpx;
  font-size: 24rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-shrink: 0;
}
.c-nav.active {
  color: #259cd6;
}
.c-body-content {
  width: 100%;
  height: auto;
}
.c-body-content .content-top {
  width: 100%;
  height: 53rpx;
  background: #f5f5f5;
  font-size: 24rpx;
  color: #999;
  padding-left: 26rpx;
  line-height: 53rpx;
}
.c-body-content .content-bottom {
  width: 100%;
  height: auto;
  padding-right: 25rpx;
}
.c-body-content .c-item {
  width: 538rpx;
  height: 206rpx;
  border-bottom: 2rpx solid #f4f4f4;
}
.c-body-content .c-item-picture {
  width: 170rpx;
  height: 170rpx;
  float: left;
}
.c-item-picture image {
  width: 150rpx;
  height: 150rpx;
  margin-top: 15rpx;
  margin-left: 10rpx;
}
.c-body-content .c-item-right {
  width: calc(100% - 170rpx);
  height: 100%;
  float: left;
}
.c-item-name {
  font-size: 28rpx;
  line-height: 37rpx;
  color: #181818;
  margin-top: 16rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.c-item-desc {
  font-size: 24rpx;
  line-height: 33rpx;
  color: #929292;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.c-item-other {
  width: 100%;
  height: 90rpx;
  margin-top: 27rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.c-item-price {
  font-size: 26rpx;
  color: #929292;
  display: flex;
  justify-content: left;
}
.c-price-num {
  color: #ef3947;
}
.c-item-other .shop {
  width: 47rpx;
  height: 47rpx;
  background: url(data:image/png;base64,)no-repeat center center /100% auto;
  position: relative;
}
.c-item-mask{
  width: 20rpx;
  height: 20rpx;
  background: #259cd6;
  border-radius: 50%;
  position: absolute;
  top:30%;
  left:30%;
  transition: all 1s;
  z-index:-1;
}