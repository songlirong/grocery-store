.footer {
  width: 100%;
  height: 100rpx;
  background: #0ac5ff;
  position: fixed;
  bottom: 0;
  left: 0;
  color: #fff;
  text-align: center;
  line-height: 100rpx;
}
button::after{
  border:none;
}
.di {
  width: 100%;
  height: 270rpx;
  background: #f5f5f5;
  display: flex;
  justify-content: center;
  padding-top: 48rpx;
  box-sizing: border-box;
}
.di .xian {
  width: 136rpx;
  height: 2rpx;
  background: #bcbcbc;
  margin-top: 20rpx;
}
.di .xian:nth-child(1) {
  margin-right: 20rpx;
}
.di text {
  margin-right: 20rpx;
}
.di text {
  font-size: 28rpx;
  color: #bcbcbc;
}
.d-top {
  width: 100%;
  height: auto;
  border-bottom: 2rpx solid #fafafa;
  background: #fff;
}
.d-top-fan {
  width: 72rpx;
  height: 72rpx;
  border-radius: 50%;
  background: #b2b2b2 url(data:image/png;base64,) no-repeat center center / 30rpx 30rpx;
  position: fixed;
  margin-left:17rpx;
  margin-top:58rpx;
  z-index: 999;
  transition: all 1.5s;
}
.d-top-fan.active{
  background: #fff url(data:image/png;base64,)no-repeat center center  /30rpx 30rpx;
}
.d-top-header {
  width: 100%;
  height: 50rpx;
  background: #999;
  opacity: 0.7;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 999;
  transition: all 1.5s;
}
.d-top-header.active {
  opacity: 1;
}
.d-top-image {
  width: 100%;
  height: 750rpx;
  background: #3897d1;
  position: relative;
  margin-bottom: 16rpx;
}
.d-top-image swiper {
  width: 100%;
  height: 100%;
}
.swiper-image {
  width: 100%;
  height: 100%;
  background: red;
}
.d-top-image-num {
  width: 88rpx;
  height: 42rpx;
  background: #b2b1b2;
  border-radius: 21rpx;
  position: absolute;
  bottom: 45rpx;
  right: 0;
  font-size: 20rpx;
  color: white;
  line-height: 42rpx;
  text-align: center;
  z-index: 99;
}
.d-top-middle {
  width: 100%;
  height: 135rpx;
  display: flex;
  justify-content: space-between;
  padding: 0 26rpx;
  box-sizing: border-box;
}
.d-top-text {
  width: 578rpx;
  height: 100%;
}
.share {
  color: #757575;
  font-size: 26rpx;
  margin-top: 8rpx;
  background: white;
}
.d-top-title {
  line-height: 60rpx;
  font-size: 38rpx;
  color: #2b2b2b;
  font-weight: 600;
  display: block;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  margin-bottom: 10rpx;
}
.d-top-desc {
  width: 100%;
  height: 76rpx;
  margin-top: 12rpx 0 13rpx;
  line-height: 38rpx;
  font-size: 30rpx;
  color: #989898;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.share-icon {
  width: 44rpx;
  height: 44rpx;
  margin-bottom: 12rpx;
  background: url(data:image/png;base64,)no-repeat center center/100% auto;
}
.d-top-group {
  width: 100%;
  height: 100rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.d-top-price {
  width: 80%;
  height: 100rpx;
  padding-left: 26rpx;
  box-sizing: border-box;
  line-height: 100rpx;
}
.d-top-shop {
  width: 12%;
  height: 100rpx;
}
.d-group-shop {
  height: 100rpx;
  position: relative;
  background: url(data:image/png;base64,)no-repeat center center /50rpx 50rpx;
}
.shop2 {
  background: url(data:image/png;base64,) no-repeat center center /50rpx 50rpx;
}
.d-top-price text:first-child {
  font-size: 26rpx;
  color: #fe4544;
}
.d-top-price text:nth-child(2) {
  font-size: 36rpx;
  color: #fe4544;
}
.d-top-price text:nth-child(3) {
  margin-left: 5rpx;
  font-size: 26rpx;
  color: #848484;
}
.d-top-time {
  width: 100%;
  height: 100rpx;
  padding-left: 26rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  border-top: 1rpx solid #fafafa;
}
.top-time-img {
  width: 53rpx;
  height: 42rpx;
  background-image: url(data:image/png;base64,);
  background-position: 0 0;
  background-size: 53rpx 42rpx;
}
.top-time-text {
  font-size: 26rpx;
  color: #000;
  margin-left: 24rpx;
}
.d-top-selected {
  width: 100%;
  height: 130rpx;
  border-top: 20rpx solid #f5f5f5;
  border-bottom: 20rpx solid #f5f5f5;
  padding: 0 26rpx;
  box-sizing: border-box;
  font-size: 28rpx;
  color: #121212;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.top-selected-icon {
  width: 14rpx;
  height: 21rpx;
  background-image: url(data:image/png;base64,);
  background-size: 14rpx 21rpx;
  background-position: 0 0;
}
.d-top-details {
  width: 100%;
  height: auto;
  border-bottom: 2rpx solid #f6f6f6;
}
.top-details-header {
  width: 100;
  height: 90rpx;
  color: #121212;
  font-size: 30rpx;
  border-bottom: 2rpx solid #f6f6f6;
  text-align: center;
  line-height: 90rpx;
}
.top-details-bottom {
  width: 100%;
  height: auto;
  padding: 0 26rpx;
  box-sizing: border-box;
  line-height: 90rpx;
}
.details-bottom-item {
  width: 100%;
  height: 90rpx;
  font-size: 28rpx;
  color: #131313;
}
.details-bottom-item .bottom-item1 {
  width: 200rpx;
  float: left;
}
.num-mark {
  width: 35rpx;
  height: 35rpx;
  border-radius: 50%;
  background: #ff4432;
  position: absolute;
  right: 10rpx;
  top: 10rpx;
  color: white;
  font-size: 22rpx;
  font-weight: bold;
  text-align: center;
  line-height: 35rpx;
}
.header {
  width: 100%;
  height: 186rpx;
  background: white;
  position: fixed;
  top: 50rpx;
  left: 0;
  z-index: 998;
  opacity: 0;
  transition: all 1.5s;
}
.header.active {
  opacity: 1;
}
.header-top {
  height: 92rpx;
  width: 100%;
  padding-left: 17rpx;
  border-bottom: 1rpx solid #d5d5d5;
  display: flex;
  align-items: center;
}
.header-top-title {
  width: 500rpx;
  height: 100%;
  padding-left:72rpx;
  box-sizing: border-box;
  line-height: 92rpx;
  font-size: 36rpx;
  font-weight: 500;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.header-title-list {
  width: 100%;
  height: 92rpx;
  border-bottom: 1rpx solid #d5d5d5;
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.header-title-name {
  width: 118rpx;
  height: 100%;
  color: #686868;
  font-size: 26rpx;
  line-height: 92rpx;
  text-align: center;
}
.header-title-name.active {
  color: #50b6ff;
  position: relative;
}
.header-title-name.active::after{
  content: '';
  display: block;
  width: 118rpx;
  height: 5rpx;
  background: #23a3ff;
  position: absolute;
  bottom:0;
}
