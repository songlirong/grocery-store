/* pages/goods/pages/result/result.wxss */

.result {
  width: 100%;
  height: auto;
  background: #fff;
}

.search-title {
  width: 100%;
  height: 145rpx;
  border-bottom: 2rpx solid #f6f6f6;
  background: #bdbdbd;
  padding-top: 40rpx;
  box-sizing: border-box;
}

.search-title-content {
  width: 100%;
  height: 100%;
  background: #fff;
  padding: 0 24rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.search-back {
  width: 35rpx;
  height: 38rpx;
  background: url(data:image/png;base64,) no-repeat center / 35rpx auto;
}

.search-input {
  width: 400rpx;
  height: 63rpx;
  background: #f5f5f5;
  border-radius: 32rpx;
  padding:0 20rpx;
  box-sizing: border-box;
  font-size: 26rpx;
  color: #202020;
  position: relative;
}
.search-input-text{
  width: 310rpx;
  height: 63rpx;
}
.search-input-icon {
  width: 72rpx;
  height: 100%;
  position: absolute;
  top: 0;
  right:0;
  z-index: 999;
  background: url(data:image/png;base64,) no-repeat center / 30rpx auto;
}

.search-btn {
  width: 114rpx;
  height: 100%;
}

.result-cate {
  width: 100%;
  height: 72rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
  position: relative;
}

.result-category {
  width: auto;
  height: 100%;
  padding: 0 24rpx;
  font-size: 24rpx;
  color: #999;
  text-align: center;
  line-height: 72rpx;
}

.result-span {
  width: 2rpx;
  height: 32rpx;
  background: #e3e3e3;
}

.cate-icon {
  width: 0;
  height: 0;
  border-width: 10rpx 10rpx 0;
  border-style: solid;
  border-color: #999 transparent transparent;
  float: right;
  margin: 31rpx 0 0 24rpx;
}

.cate-icon-top {
  width: 0;
  height: 0;
  border-width: 0 10rpx 10rpx;
  border-style: solid;
  border-color: transparent transparent #999;
  float: right;
  margin: 31rpx 0 0 24rpx;
}

.cate-icon-right {
  width: 0;
  height: 0;
  border-width: 10rpx 0 10rpx 10rpx;
  border-style: solid;
  border-color: transparent transparent transparent #999;
  float: right;
  margin: 26rpx 0 0 24rpx;
}

.cate-list {
  width: 100%;
  height: auto;
  background: #fff;
  position: absolute;
  top: 72rpx;
  left: 0;
}

.cate-li {
  width: 100%;
  height: 84rpx;
  border-top: 1rpx solid #f0f0f0;
  display: flex;
  justify-content: space-between;
  padding: 0 48rpx;
  box-sizing: border-box;
  font-size: 24rpx;
  color: #999;
  text-align: center;
  line-height: 84rpx;
}

.cate-li:nth-child(1) {
  border-bottom: 1rpx solid #f0f0f0;
}

.cate-li-icon {
  width: 28rpx;
  height: 100%;
  background: url(data:image/png;base64,) no-repeat center center / 28rpx auto;
}

.result-content {
  width: 100%;
  height: auto;
  background: #f5f5f5;
  padding-top: 7rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}

.guess-list {
  width: 100%;
  height: auto;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  background: #f5f5f5;
}

.guess-item {
  width: 375rpx;
  height: 506rpx;
  background: #fff;
  border-bottom: 6px solid #f5f5f5;
  padding: 20rpx 30rpx 0 20rpx;
  box-sizing: border-box;
}

.guess-item:nth-child(n) {
  border-right: 6px solid #f5f5f5;
}

.guess-item:nth-child(2n) {
  border-right: none;
}

.item-img {
  width: 100%;
  height: 240rpx;
  background: #09afff;
}

.item-img image {
  width: 100%;
  height: 100%;
}

.item-content {
  width: 100%;
}

.item-names {
  width: 100%;
  margin-top: 20rpx;
  color: #4c4c4c;
  font-size: 28rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}

.item-discount {
  width: 100%;
  height: 44rpx;
}

.discount {
  width: 90%;
  height: 30rpx;
  color: #bdbdbd;
  font-size: 22rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.dis-price {
  width: 100%;
  line-height: 38rpx;
  font-size: 20rpx;
  color: #dadada;
  text-align: left;
  text-decoration: line-through;
}

.price {
  width: 100%;
  height: 60rpx;
  display: flex;
  justify-content: space-between;
  align-items: flex-start;
}

.price .num {
  width: auto;
  line-height: 50rpx;
  font-size: 24rpx;
  color: #fe2827;
}

.price .num1 {
  color: #333;
}

.price .num2 {
  font-size: 22rpx;
}

.price .num2 {
  font-size: 22rpx;
  color: #959595;
}

.price .num-text {
  width: auto;
  height: 100%;
  display: flex;
  justify-content: flex-start;
  align-items: flex-start;
}

.car-logo {
  width: 60rpx;
  height: 60rpx;
  background: url('data:image/png;base64,') no-repeat right center / 48rpx auto;
}
