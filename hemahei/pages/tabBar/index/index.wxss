.bgc {
  width: 750rpx;
  height: auto;
  background: #f5f5f5;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

#index-title {
  width: 100%;
  height: 138rpx;
  background: #0ac7ff;
  border-bottom: 1rpx solid #8ee1ff;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  padding: 40rpx 24rpx 0;
  box-sizing: border-box;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 999;
}

.location {
  width: 75rpx;
  height: 100%;
  background: url(data:image/png;base64,) no-repeat left center / auto 42rpx;
}

.search {
  width: 420rpx;
  height: 63rpx;
  background-color: #fff;
  padding-left: 77rpx;
  box-sizing: border-box;
  border-radius: 31rpx;
  position: relative;
  font-size: 26rpx;
  color: #999;
  line-height: 63rpx;
}

.search .icon-search {
  position: absolute;
  left: 26rpx;
  top: 17rpx;
  width: 28rpx;
  height: 28rpx;
  background: url(data:image/png;base64,) no-repeat center / 100% auto;
}

.banner {
  width: 100%;
  height: 412rpx;
  padding: 150rpx 24rpx 0;
  box-sizing: border-box;
  overflow: hidden;
  margin: 0 auto;
  background: #fff;
}

.swiper-image {
  width: 702rpx;
  height: 262rpx;
  background: #0ac7ff;
}

#category {
  width: 100%;
  height: 360rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  flex-wrap: wrap;
  padding: 0 24rpx;
  box-sizing: border-box;
  background: #fff;
}

.one-of-category {
  width: 140rpx;
  height: 180rpx;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
  padding-top: 20rpx;
  box-sizing: border-box;
}

.picture {
  width: 100rpx;
  height: 100rpx;
  background: #e1e1e1;
  border-radius: 50%;
}

.category-text {
  width: 100rpx;
  font-size: 24rpx;
  color: #515151;
  margin-top: 20rpx;
  text-align: center;
}

.big-mouth-bar {
  width: 100%;
  padding: 24rpx 24rpx 0;
  height: 132rpx;
  box-sizing: border-box;
  display: flex;
  background: #fff;
  position: relative;
}

.mask {
  width: 100%;
  height: 132rpx;
  position: absolute;
  top: 0;
  left: 0;
  background: rgba(255, 255, 255, 0.01);
}

.bar-logo {
  width: 109rpx;
  height: 109rpx;
  background: url(data:image/jpeg;base64,) no-repeat center / 100% auto;
}

.bar-swiper {
  width: calc(100% - 109rpx);
  height: 108rpx;
}

.bar-content {
  width: 100%;
  height: 100%;
  margin-left: 10rpx;
  padding: 0 17rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.content-img {
  width: 94rpx;
  height: 70rpx;
  background: #0ac7ff;
}

.content-text {
  width: 364rpx;
  height: 70rpx;
  font-size: 24rpx;
  color: #5e5e5e;
  line-height: 34rpx;
}

.fu {
  float: left;
}

.content-text-h3 {
  font-size: 24rpx;
  font-weight: 700;
  float: left;
}

.content-text-span {
  width: 2rpx;
  height: 24rpx;
  background: #ededed;
  margin: 8rpx 10rpx 0;
  float: left;
}

.worthwhile {
  width: 100%;
  height: 428rpx;
  background: #fff;
}

.worthwhile-title {
  width: 100%;
  height: 92rpx;
  padding: 0 24rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.worthwhile-title .title-left, .title-right {
  width: auto;
  height: 100%;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}

.worthwhile-title .title-logo {
  width: 120rpx;
  height: 100%;
  background: url(data:image/jpeg;base64,) no-repeat center / 100% auto;
}

.worthwhile-title .title-text {
  font-size: 24rpx;
  color: #000;
  line-height: 92rpx;
  margin-right: 14rpx;
}

.worthwhile-title .title-VIP {
  width: 81rpx;
  height: 100%;
  background: url(data:image/jpeg;base64,) no-repeat center / 100% auto;
}

.worthwhile-title .title-more {
  font-size: 22rpx;
  color: #8f8f8f;
  line-height: 92rpx;
}

.worthwhile-title .title-arrow {
  width: 20rpx;
  height: 100%;
  background: url(data:image/png;base64,) no-repeat center / 100% auto;
}

.worthwhile-scroll {
  width: 750rpx;
  height: 365rpx;
  overflow: hidden;
}

.worthwhile-content {
  width: auto;
  height: 365rpx;
  padding-left: 24rpx;
  box-sizing: border-box;
  display: flex;
  flex-wrap: nowrap;
}

.worthwhile-content .look-at-more {
  height: 304rpx;
}

.worthwhile-content-list {
  width: 625rpx;
  height: 304rpx;
  background: #f7f7f7;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 20rpx;
  box-sizing: border-box;
  border-radius: 5rpx;
  margin-right: 20rpx;
}

.list-img {
  width: 230rpx;
  height: 230rpx;
  background: #0ac7ff;
}

.list-information {
  width: calc(100% - 230rpx);
  height: 100%;
  margin-left: 20rpx;
  padding-top: 37rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
}

.information-h3 {
  height: 60rpx;
  font-size: 28rpx;
  color: #101010;
  line-height: 1.1;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.information-span {
  font-size: 22rpx;
  color: #acacac;
  line-height: 1.2;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.information-feature {
  width: 100%;
  height: 76rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  margin: 25rpx 0;
}

.feature-identification {
  width: auto;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: flex-start;
}

.identification-h3 {
  font-size: 32rpx;
  color: #101010;
  line-height: 1.5;
}

.feature-vertical {
  width: 2rpx;
  height: 66rpx;
  margin: 0 35rpx;
  background: #e6e6e6;
}

.information-price {
  width: 100%;
  height: auto;
  display: flex;
  justify-content: space-between;
  line-height: 46rpx;
}

.information-price-num {
  font-size: 22rpx;
  color: #8b8b8b;
}

.price-span {
  display: inline;
  font-size: 24rpx;
  color: #ff2828;
  font-weight: 500;
}

.information-price-cart {
  width: 46rpx;
  height: 46rpx;
  border-radius: 50%;
  background: #09afff url(data:image/png;base64,) no-repeat center center / 30rpx auto;
}

#best {
  width: 100%;
  height: auto;
  padding: 0 24rpx;
  box-sizing: border-box;
}

.best-title {
  width: 100%;
  height: 66rpx;
  text-align: center;
  line-height: 66rpx;
  font-size: 24rpx;
  color: #999;
  font-weight: bold;
  position: relative;
}

.best-title text {
  color: #ed6a35;
}

.best-title::before {
  content: '';
  display: block;
  width: 80rpx;
  height: 1rpx;
  background: #d4d4d4;
  position: absolute;
  top: 32rpx;
  left: 196rpx;
}

.best-title::after {
  content: '';
  display: block;
  width: 80rpx;
  height: 1rpx;
  background: #ed6a35;
  position: absolute;
  top: 32rpx;
  right: 196rpx;
}

.best-content {
  width: 100%;
  height: 611rpx;
  padding-top: 25rpx;
  box-sizing: border-box;
  background: #fff;
}

.content-picture {
  width: 702rpx;
  height: 222rpx;
  border-radius: 111rpx;
  background: red;
  display: block;
  margin: 0 auto;
}

.content-goods {
  width: 750rpx;
  height: 364rpx;
  overflow: hidden;
}

.content-goods-list {
  width: auto;
  height: 100%;
  display: flex;
  padding: 29rpx 0 29rpx 24rpx;
  box-sizing: border-box;
  justify-content: flex-start;
  align-items: center;
  flex-wrap: nowrap;
}

.best-goods {
  width: 206rpx;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  margin-right: 20rpx;
}

.look-at-more {
  width: 80rpx;
  height: 100%;
  background: #f7f7f7;
  display: flex;
  flex-direction: column;
  justify-content: center;
  position: sticky;
  left: 670rpx;
}

.more-text {
  width: 24rpx;
  height: auto;
  line-height: 1.2;
  white-space: pre-wrap;
  font-size: 24rpx;
  color: #999;
  padding-right: 26rpx;
  margin-left: 20rpx;
}

.more-icon {
  width: 24rpx;
  height: 24rpx;
  border: 1rpx solid #acacac;
  border-radius: 50%;
  margin: 20rpx 0 0 20rpx;
  background: url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABQAAAAUCAYAAACNiR0NAAAAm0lEQVQ4T63UvRHDIAwFYEmTeZM4FVBllXSCys4kGQ0fRZqcDe/5oOY+gf5UJh+d7Mkl6O5rSmlnA56C7r6Y2bfWuscYnwzafaGZbSzazWH7NosOi8KiQ7Dlj0EhkEFhEEVvgSLyCSGsZ+0Eg7889rAWAAJzzm9VfY0wCCyltPF7INgQZLEueAe7BNECUFWeur6YdfV/F2obJsABO4FjFXz/oK8AAAAASUVORK5CYII=) no-repeat center / 20rpx auto;
}

.best-goods-img {
  width: 206rpx;
  height: 206rpx;
  background: #0ac7ff;
}

.best-goods-name {
  font-size: 24rpx;
  color: #101010;
  line-height: 1;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.best-goods-price {
  width: 100%;
  height: auto;
  display: flex;
  justify-content: space-between;
}

.goods-price-num {
  font-size: 18rpx;
  color: #8b8b8b;
  line-height: 38rpx;
}

.goods-price-cart {
  width: 38rpx;
  height: 38rpx;
  border-radius: 50%;
  background: #09afff url(data:image/png;base64,) no-repeat center center / 24rpx auto;
}

.get_into{
  width: 100%;
  height: 100%;
  background: #f0f0f0;
  display: flex;
  justify-content: center;
  align-items: center;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 999;
}
.get_userinfo{
  width: 500rpx;
  height: 80rpx;
  border-radius: 40rpx;
  background: #1269db;
  color: #fff;
  font-size: 38rpx;
  text-align: center;
  line-height: 80rpx;
}