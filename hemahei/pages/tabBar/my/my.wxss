.body {
  width: 750rpx;
  height: auto;
  background-color: #f5f5f5;
  position: relative;
}

.header {
  width: 100%;
  height: 250rpx;
  background: linear-gradient(to right, #0acaff, #0ab0ff);
}

.header-img {
  width: 44rpx;
  height: 44rpx;
  position: fixed;
  left: 37rpx;
  top: 74rpx;
  background: url('data:image/png;base64,') no-repeat center center / 44rpx 44rpx;
  z-index: 999;
}

.gai {
  width: 100%;
  height: 150rpx;
  background: linear-gradient(to right, #0acaff, #0ab0ff);
  position: fixed;
  top: 0;
  left: 0;
  z-index: 998;
}

.message {
  width: 700rpx;
  height: 230rpx;
  background: #fff;
  border-radius: 20rpx;
  position: absolute;
  left: 25rpx;
  top: 174rpx;
  border-bottom: 15rpx solid #0ab0ff;
  z-index: 889;
}

.my-head {
  width: 125rpx;
  height: 125rpx;
  border-radius: 50%;
  overflow: hidden;
  background: #ececec;
  position: absolute;
  left: 27rpx;
  top: -18rpx;
  float: left;
}

.my-head image {
  width: 125rpx;
  height: 125rpx;
}

.my-name {
  width: 340rpx;
  height: 90rpx;
  margin-left: 152rpx;
  padding-left: 30rpx;
  box-sizing: border-box;
  font-size: 26rpx;
  line-height: 90rpx;
  color: #00c6ff;
  float: left;
}

.my-code {
  width: 206rpx;
  height: 90rpx;
  float: right;
}

.code-one {
  width: 40rpx;
  height: 40rpx;
  background: url('data:image/png;base64,') no-repeat center center / 40rpx 40rpx;
  float: left;
  margin-top: 22rpx;
  margin-left: 34rpx;
}

.code-two {
  width: 40rpx;
  height: 40rpx;
  background: url('data:image/png;base64,') no-repeat center center / 40rpx 40rpx;
  float: right;
  margin-top: 22rpx;
  margin-right: 34rpx;
}

.my-ticket {
  width: 100%;
  height: 116rpx;
  margin-top: 114rpx;
  display: flex;
  align-items: center;
  justify-content: space-around;
}

.my-ticket .coupon, .giftcard, .electronic-coupon {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-around;
}

.my-ticket .text-number {
  height: 28rpx;
  font-weight: bold;
  font-size: 28rpx;
  color: #333;
  margin-bottom: 10rpx;
}

.my-ticket .text-word {
  font-size: 24rpx;
  color: #acacac;
}

.order {
  width: 100%;
  height: 230rpx;
  background: #fff;
  padding-top: 186rpx;
}

.order-top {
  width: 100%;
  height: 80rpx;
  padding: 0 25rpx;
  box-sizing: border-box;
}

.order-top-box {
  width: 100%;
  height: 100%;
  border-bottom: 1rpx solid #ececec;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.more-order {
  width: 170rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.my-order-text {
  font-size: 30rpx;
  font-weight: bold;
}

.more-order-text {
  font-size: 24rpx;
  color: #989898;
}

.more-img {
  width: 12rpx;
  height: 24rpx;
  background: url('data:image/png;base64,') no-repeat center center / 12rpx 24rpx;
}

.bottom-content .img-fu {
  background: url('data:image/png;base64,');
}

.bottom-content .img-pei {
  background: url('data:image/png;base64,');
}

.bottom-content .img-shou {
  background: url('data:image/png;base64,');
}

.bottom-content .img-ping {
  background: url('data:image/png;base64,');
}

.bottom {
  width: 100%;
  height: calc(100% - 80rpx);
  display: flex;
}

.tool-bottom {
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}

.tool-bottom-one {
  display: flex;
}

.tool-bottom-two {
  display: flex;
}

.bottom .bottom-content {
  width: 25%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.bottom-content .image {
  width: 50rpx;
  height: 50rpx;
  margin-bottom: 10rpx;
  background-size: 50rpx 50rpx;
  background-position: 0 0;
}

.bottom-content text {
  font-size: 24rpx;
  color: #898989;
}

.bottom-content .orange {
  font-size: 20rpx;
  color: orange;
  margin-top: 10rpx;
}

.tool {
  width: 100%;
  height: 406rpx;
  margin-top: 18rpx;
  background: #fff;
}

.tool-top {
  width: 100%;
  height: 80rpx;
  padding: 0 25rpx;
  box-sizing: border-box;
}

.tool-top-box {
  width: 100%;
  height: 100%;
  border-bottom: 1rpx solid #ececec;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.tool-top-box text {
  font-size: 30rpx;
  font-weight: bold;
}

.bottom-content .img-yao {
  background: url('data:image/png;base64,') no-repeat center center /50rpx 50rpx;
}

.bottom-content .img-dian {
