page {
  background: #f5f5f5;
}
.invoice-box {
  widows: 100%;
  height: auto;
}
.invoice-header {
  width: 100%;
  height: 144rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 20rpx;
  box-sizing: border-box;
  border-bottom: 1rpx solid #e8e8e8;
  background: #fefefe;
  margin-bottom: 18rpx;
}
.back1 {
  width: 37rpx;
  height: 37rpx;
}
.back {
  width: 37rpx;
  height: 37rpx;
  background: url('data:image/png;base64,') no-repeat 5rpx 5rpx/28rpx 28rpx;
}

.invoice-cate {
  color: #000;
  font-size: 32rpx;
}

.invoice-content {
  width: 100%;
  height: 154rpx;
  padding: 30rpx 20rpx 0;
  box-sizing: border-box;
  background: #fefefe;
  margin-bottom: 20rpx;
}

.invoice-title {
  width: 100%;
  height: 26rpx;
  color: #313131;
  font-size: 26rpx;
  margin-bottom: 25rpx;
}

.invoice-type {
  width: 485rpx;
  height: 45rpx;
  display: flex;
  justify-content: space-between;
  color: #000;
  font-size: 26rpx;
  line-height: 45rpx;
}

.no-invoice {
  width: 190rpx;
  height: 100%;
  padding-left: 55rpx;
  box-sizing: border-box;
  background: url('data:image/png;base64,') no-repeat 0 0/45rpx 45rpx;
}

.no-invoice.no-choose {
  width: 190rpx;
  height: 100%;
  padding-left: 55rpx;
  box-sizing: border-box;
  background: url('data:image/png;base64,') no-repeat 0 0/45rpx 45rpx;
}

.yes-invoice {
  width: 200rpx;
  height: 100%;
  padding-left: 55rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  background: url('data:image/png;base64,') no-repeat 0 0/45rpx 45rpx;
}

.yes-invoice.no-choose {
  background: url('data:image/png;base64,') no-repeat 0 0/45rpx 45rpx;
}
.warn{
  width: 30rpx;
  height: 30rpx;
  margin: 8rpx 0 0 10rpx;
  background: url('data:image/png;base64,') no-repeat 0/100%;
}
.careful{
  width: 100%;
  height: 120rpx;
  padding: 0 20rpx;
}
.careful-title{
  width: 100%;
  height: 30rpx;
  color: #6a6a6a;
  font-size: 30rpx;
  line-height: 30rpx;
  margin-bottom: 20rpx;
  padding-left: 40rpx;
  box-sizing: border-box;
  background: url('data:image/png;base64,') no-repeat 0 /30rpx 30rpx;
}
.careful-txt{
  width: 100%;
  height: 35rpx;
  color: #6a6a6a;
  font-size: 24rpx;
  line-height: 35rpx;
}
.sure{
  width: 100%;
  height: 100rpx;
  position: fixed;
  left: 0;
  bottom: 0;
  background: #fefefe;
  display: flex;
  justify-content: center;
  align-items: center;
}
.sure-btn{
  width: 678rpx;
  height: 66rpx;
  border-radius: 33rpx;
  background: #4facf8;
  color: #ffffff;
  font-size: 30rpx;
  line-height: 66rpx;
  text-align: center;
}
