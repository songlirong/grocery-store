page {
  background: #f5f5f5;
}

.pay-box {
  width: 750rpx;
  height: auto;
}
.qwer{
  width: 100%;
  height: 40rpx;
  background: #eee;
  position: fixed;
  top: 0;
  left: 0;
}
.pay-header {
  width: 100%;
  height: 100rpx;
  margin-top: 40rpx;
  background: #fefefe;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1rpx solid #eee;
  padding: 0 20rpx;
  box-sizing: border-box;
}

.back-icon {
  width: 52rpx;
  height: 52rpx;
  background: #fff url('data:image/png;base64,') no-repeat 0 6rpx/44rpx auto;
}
.back-icons {
  width: 37rpx;
  height: 37rpx;
}

.pay-cate {
  font-size: 40rpx;
  color: #161616;
}

.pay-content {
  width: 100%;
  height: auto;
  padding-bottom:104rpx;
}

.pay-place {
  width: 100%;
  height: 120rpx;
  background: #fefefe;
  padding: 0 20rpx;
  box-sizing: border-box;
  margin-bottom: 15rpx;
}

.position-icon {
  width: 660rpx;
  height: 100%;
  font-size: 26rpx;
  color: #484848;
  line-height: 120rpx;
  font-weight: 500;
  padding-left: 40rpx;
  box-sizing: border-box;
  float: left;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  background: url('data:image/png;base64,') no-repeat 0 47rpx/30rpx 30rpx;
}


.next-icon {
  width: 40rpx;
  height: 100%;
  float: right;
  background: url('data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABgAAAAYCAYAAADgdz34AAAAj0lEQVRIS83VsQ3AIAwEQNgkI2SEjJIRwjAWbUakpEAWoksV/I+shJ4/2bLsGJxfdM4P3wI5563WWlJKha30tQIR2VX1bq0dLDJtkYicqnqxyBQYrVlBTMAKYgZYBAIYBAZQhAIQ5J8AMrZwBUj4aCUEoOEQwISbATbcBKyETwH3de1+cNgr9vwHjSkDugMdOrvAGdWNZtEAAAAASUVORK5CYII=') no-repeat 16rpx 48rpx/24rpx 24rpx;
}

.pay-shop {
  width: 100%;
  height: auto;
}

.pay-shop-item {
  width: 100%;
  height: 425rpx;
  border-bottom:1rpx dashed #333;
}

.pay-shops {
  width: 100%;
  height: 311rpx;
  background: #fefefe;
  padding: 20rpx 20rpx 0;
  box-sizing: border-box;
  position: relative;
  margin-bottom: 18rpx;
}

.pay-times {
  width: 100%;
  height: 94rpx;
  display: flex;
  justify-content: space-between;
}

.time-icon {
  width: 150rpx;
  height: 100%;
  padding-left: 40rpx;
  background: url('data:image/png;base64,') no-repeat 0 36rpx/24rpx 24rpx;
  color: #272727;
  font-size: 26rpx;
  line-height: 94rpx;
}

.pay-shops .next-icon {
  width: 165rpx;
  height: 100%;
  color: #2299f6;
  font-size: 26rpx;
  line-height: 94rpx;
  padding-right: 40rpx;
  text-align: right;
  background-position: 180rpx 36rpx;
}

.shop-item {
  width: 704rpx;
  height: 210rpx;
  margin: 0 auto;
  background: #fafafa;
  border-radius: 28rpx;
  display: flex;
  justify-content: space-around;
  padding-top: 16rpx;
  box-sizing: border-box;
}

.shop-img {
  width: 142rpx;
  height: 142rpx;
  background: #e33333;
  margin-left: 16rpx;
}
.shop-img image{
  display: block;
  width: 142rpx;
  height: 142rpx;
}
.shop-info {
  width: 550rpx;
  height: 100%;
  margin-left: 20rpx;
  font-size: 24rpx;
  color: #686868;
}

.shop-names {
  width: 310rpx;
  height: 60rpx;
  font-size: 26rpx;
  color: #1c1c1c;
  /* margin-bottom: 28rpx; */
  font-weight: 700;
}

.shop-price {
  font-size: 24rpx;
  color: #878787;
  position: absolute;
  right: 23rpx;
  bottom: 28rpx;
}

.price-num {
  display: inline;
  color: #e84138;
  font-size: 26rpx;
}

.invoice {
  width: 100%;
  height: 90rpx;
  background: #fefefe;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 20rpx;
  box-sizing: border-box;
}

.invoice-name {
  color: #b4b4b4;
  font-size: 24rpx;
}

.invoice-state {
  width: 180rpx;
  height: 100%;
  line-height: 90rpx;
  color: #242424;
  font-size: 28rpx;
  padding-right: 40rpx;
  box-sizing: border-box;
  background: url('data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABgAAAAYCAYAAADgdz34AAAAj0lEQVRIS83VsQ3AIAwEQNgkI2SEjJIRwjAWbUakpEAWoksV/I+shJ4/2bLsGJxfdM4P3wI5563WWlJKha30tQIR2VX1bq0dLDJtkYicqnqxyBQYrVlBTMAKYgZYBAIYBAZQhAIQ5J8AMrZwBUj4aCUEoOEQwISbATbcBKyETwH3de1+cNgr9vwHjSkDugMdOrvAGdWNZtEAAAAASUVORK5CYII=') no-repeat 160rpx 35rpx/24rpx 24rpx;
}

.submit {
  width: 100%;
  height: 104rpx;
  background: #fff;
  position: fixed;
  left: 0;
  bottom: 0;
  border-top: 1rpx solid #eaeaea;
  box-sizing: border-box;
}

.submit-content {
  float: right;
  width: 660rpx;
  height: 100%;
  display: flex;
  justify-content: space-between;
}

.and-price {
  width: 330rpx;
  height: 100%;
  color: #343434;
  font-size: 30rpx;
  line-height: 104rpx;
  text-align: right;
}

.and-price-item {
  color: #ff5254;
  font-size: 30rpx;
  display: inline;
}

.submit-btn {
  width: 230rpx;
  height: 100%;
  background: #0aafff;
  color: #fffffd;
  text-align: center;
  line-height: 104rpx;
  font-size: 30rpx;
}

/* 选择送达时间 */

.mask {
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.3);
  position: absolute;
  left: 0;
  top: 0;
  display: none;
}

.times {
  width: 100%;
  height: 696rpx;
  position: fixed;
  left: 0;
  bottom: 0;
  background: #fafafa;
  z-index: 999;
   display: none;
}

.times .pay-header {
  border-bottom: 1px solid #eaeaea;
}

.times-bottom {
  width: 100%;
  height: 460rpx;
  display: flex;
  justify-content: space-between;
}

.time-day {
  width: 132rpx;
  height: 100%;
  border-right: 1px solid #e9e9e9;
}

.day-one {
  width: 100%;
  height: 90rpx;
  text-align: center;
  line-height: 90rpx;
  font-size: 24rpx;
}

.day-select {
  background: #fefefe;
  width: 133rpx;
}

.selects {
  color: #3da3f7 !important;
}

.hours {
  width: calc(100% - 132rpx);
  height: 100%;
  text-align: center;
  padding-left: 30rpx;
  box-sizing: border-box;
  background: #fefefe;
  overflow: hidden;
}

.hours-item {
  width: 100%;
  height: 89rpx;
  border-bottom: 1rpx solid #e9e9e9;
  padding-right: 40rpx;
  box-sizing: border-box;
}

.time-slot, .discount {
  color: #2c2c2c;
  font-size: 24rpx;
  line-height: 89rpx;
  float: left;
  margin: 0 20rpx;
}

.sure {
  width: 100%;
  height: 100rpx;
  position: absolute;
  left: 0;
  bottom: 50rpx;
  background: #fefefe;
  display: flex;
  justify-content: center;
  align-items: center;
}

.sure-btn {
  width: 678rpx;
  height: 66rpx;
  border-radius: 33rpx;
  background: #4facf8;
  color: #fff;
  font-size: 30rpx;
  line-height: 66rpx;
  text-align: center;
}
