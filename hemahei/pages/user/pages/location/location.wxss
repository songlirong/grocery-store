.select-location {
  width: 100%;
  min-height: 1334rpx;
  background: #f5f5f5;
}

.location-title {
  width: 100%;
  height: 233rpx;
  background: #bdbdbd;
  padding-top: 40rpx;
  box-sizing: border-box;
  border-bottom: 2rpx solid #dad8d7;
  position: fixed;
  top: 0;
  left: 0;
}

.location-title-head {
  width: 100%;
  height: 104rpx;
  background: #fff;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 24rpx;
  box-sizing: border-box;
}

.title-head-back {
  width: 56rpx;
  height: 104rpx;
  background: url(data:image/png;base64,) no-repeat center center  / auto 36rpx;
}

.title-head-text {
  width: auto;
  line-height: 104rpx;
  font-size: 36rpx;
  color: #2a2a2a;
}

.location-title-feed {
  width: 100%;
  height: 90rpx;
  background: #fff;
  padding: 0 24rpx;
  box-sizing: border-box;
  position: relative;
}

.location-title-search {
  width: 100%;
  height: 62rpx;
  border-radius: 31rpx;
  padding-left: 80rpx;
  box-sizing: border-box;
  background: #f5f5f5;
}

.location-icon {
  width: 80rpx;
  height: 62rpx;
  position: absolute;
  top: 0;
  left: 24rpx;
  background: url(data:image/png;base64,) no-repeat 30rpx center / auto 34rpx;
}

.now-location {
  width: 100%;
  height: 83rpx;
  background: #e6f5ff;
  padding: 0 24rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.now-location-icon {
  width: 48rpx;
  height: 100%;
  margin-right: 20rpx;
  background: url(data:image/png;base64,) no-repeat left center / 36rpx auto;
}

.now-location-text {
  width: auto;
  line-height: 83rpx;
  font-size: 24rpx;
  color: #0092ff;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.my-location, .near-location {
  width: 100%;
  height: auto;
}

.location-text {
  width: 100%;
  height: 83rpx;
  font-size: 24rpx;
  color: #757575;
  line-height: 83rpx;
  margin-left: 24rpx;
}

.my-location-information {
  width: 100%;
  height: 132rpx;
  background: #fff;
  display: flex;
  justify-content: space-between;
  padding: 0 24rpx;
  box-sizing: border-box;
  margin-bottom: 20rpx;
}

.information-text {
  width: 606rpx;
  height: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  padding: 14rpx 0;
  box-sizing: border-box;
}

.text-name {
  font-size: 26rpx;
  color: 0b0b0b;
  line-height: 40rpx;
  margin-bottom: 22rpx;
}

.text-location {
  font-size: 24rpx;
  color: #949494;
  line-height: 32rpx;
}

.information-select {
  width: 96rpx;
  height: 100;
  background: url(data:image/png;base64,) no-repeat center / 28rpx auto;
}

.near-location-list {
  width: 100;
  height: auto;
  padding: 0 24rpx;
  box-sizing: border-box;
  background: #fff;
}

.near-location-li {
  width: 100%;
  height: 92rpx;
  font-size: 24rpx;
  color: #959595;
  line-height: 92rpx;
}

.new-location {
  width: 143rpx;
  height: 100%;
  float: right;
  font-size: 24rpx;
  line-height: 92rpx;
  color: #0092ff;
}

.new-location text {
  float: right;
}

.new-location-icon {
  float: left;
  width: 30rpx;
  height: 92rpx;
  background: url(data:image/png;base64,) no-repeat center / 100% auto;
}

.near-location-more {
  color: #151515;
}

.near-location-more-icon {
  width: 25rpx;
  height: 100%;
  float: right;
  background: url(data:image/png;base64,) no-repeat center / 25rpx auto;
}

.add-location {
  width: 684rpx;
  height: 80rpx;
  border-radius: 40rpx;
  background: #0092ff;
  font-size: 34rpx;
  color: #fff;
  margin: 30rpx auto;
  text-align: center;
  line-height: 80rpx;
}
.no-address{
  width: 100%;
  height: 200rpx;
  text-align: center;
  line-height: 200rpx;
  color: #0092ff;
  font-size: 38rpx;
}