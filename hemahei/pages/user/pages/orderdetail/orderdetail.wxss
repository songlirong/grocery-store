view {
  box-sizing: border-box;
}
.order-header {
  width: 100%;
  height: 154rpx;
  background: #bebebe;
  box-sizing: border-box;
}

.order-header .top {
  width: 100%;
  height: 40rpx;
}

.order-header .head {
  width: 100%;
  height: 114rpx;
  background: #fff;
  border-bottom: 1rpx solid #e5e5e5;
  text-align: center;
  line-height: 114rpx;
  padding: 0 50rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.order-header .head .zuo {
  width: 30rpx;
  height: 35rpx;
  background: url('data:image/png;base64,') no-repeat center center / 30rpx 35rpx;
}

.order-header .head text {
  font-size: 40rpx;
  color: #080808;
  font-weight: 550;
}

.order-header .head .you {
  width: 28rpx;
  height: 40rpx;
}
#shop {
  width: 35rpx;
  height: 35rpx;
  margin-left:25rpx;
  background: url(data:image/png;base64,)no-repeat center center /100% auto;
  position: relative;
}
.c-middle{
  width: 100%;
  height: 250rpx;
  background: linear-gradient(to right, #0acaff, #0ab0ff);
}
.c-middle-content{
  width: 590rpx;
  height: 100%;
  margin:0 auto;
  display: flex;
  justify-content: space-between;
  align-items: center;

}
.c-content-left{
  width: 380rpx;
  height: 94rpx;
 text-align: center;
 color: #fff;
 display: flex;
 flex-direction: column;
 justify-content: space-between;
}
.c-content-top{
  font-size: 38rpx;
  font-weight: bold;
  line-height: 40rpx;
}
.c-content-bottom{
  font-size: 22rpx;
  line-height: 24rpx;
}
.c-content-right{
  width: 200rpx;
  height: 100%;
  background:url(data:image/png;base64,)no-repeat center center /90% auto;
}
.pay-content {
  width: 100%;
  height: auto;
  background: #f5f5f5;
}

.pay-place {
  width: 100%;
  height: 152rpx;
  background: #fefefe;
  padding: 0 20rpx;
  box-sizing: border-box;
  margin-bottom: 19rpx;
}
.pay-place-top{
width: 100%;
height: 80rpx;
display: flex;
justify-content: space-between;
}
.pay-place-bottom{
width: 100%;
height: 72rpx;
}
.position-icon {
  width: 44rpx;
  font-size: 26rpx;
  background: url('data:image/png;base64,') no-repeat 0 20rpx/30rpx 30rpx;
}
.position-detail{
  width: 450rpx;
  height: 100%;
  font-size: 28rpx;
  color: #343434;
  line-height: 80rpx;
  font-weight: bolder;
}
.position-tel{
  width: 180rpx;
  height: 100%;
  font-size: 26rpx;
  color: #343434;
  line-height: 80rpx;
  font-weight: bolder;
}
.position-bottom-detail{
  width: 450rpx;
  margin-left:60rpx;
  line-height: 36rpx;
  font-size: 24rpx;
  color: #919191;
}
.pay-shop {
  width: 100%;
  height: auto;
}

.pay-shop-item {
  width: 100%;
}

.pay-shops {
  width: 100%;
  background: #fefefe;
  padding:0 20rpx;
  box-sizing: border-box;
  position: relative;
  overflow: hidden;
  margin-bottom:20rpx;
}
.shop-item {
  width: 704rpx;
  margin: 20rpx auto;
  padding:24rpx  20rpx;
  background: #fafafa;
  border-radius: 28rpx;
  box-sizing: border-box;
  display: flex;
  position: relative;
}

.shop-img {
  width: 125rpx;
  height: 125rpx;
  background: #e33333;
  border-radius: 20rpx;
}
.shop-img image{
  display: block;
  width: 125rpx;
  height: 125rpx;
}
.shop-info {
  width: 340rpx;
  height: 100%;
  margin-left: 20rpx;
  font-size: 24rpx;
  color: #686868;
}

.shop-names {
  width: 310rpx;
  margin-top:3rpx;
  font-size: 26rpx;
  line-height: 40rpx;
  color: #292929;
  font-weight: 700;
  margin-bottom:20rpx;
}
.shop-desc{
width: 100%;
white-space: nowrap;
overflow: hidden;
text-overflow: ellipsis;
font-size: 22rpx;
color: #8b8b8b;
}
.shop-price {
  width: 150rpx;
  float: right;
  text-align: right;
  font-size: 24rpx;
  color: #1e1e1e;
  font-weight: 700;
}
.shop-one{
  line-height: 40rpx;
  font-size: 22rpx;
  color: #8b8b8b;
}
.shop-number{
 line-height: 40rpx;
 font-size: 22rpx;
 color: #8b8b8b;
 margin-bottom:40rpx;
}
.shop-back{
 width: 160rpx;
 height: 42rpx;
 border:2rpx solid #dcdcdc;
 border-radius: 21rpx;
 position: absolute;
 bottom:25rpx;
 right:25rpx;
 font-size: 22rpx;
 color: #cecece;
 line-height: 42rpx;
 text-align: center;
}
.pay-money{
  width: 100%;
  height: 224rpx;
  padding:0 25rpx;
  box-sizing: border-box;
  background:#fff; 
  margin-bottom:40rpx;
  font-weight: bold;
}
.pay-money-content{
  width: 100%;
  height: 100%;
  border-bottom:2rpx solid #d9d9d9;
}
.pay-money-top{
  width: 100%;
  height: 134rpx;
  border-bottom:1rpx solid #d9d9d9;
}
.pay-money-item{
  width: 100%;
  height: 67rpx;
  font-size: 22rpx;
  line-height: 67rpx;
  display: flex;
  justify-content: space-between;
}
.pay-money-bottom{
  width: 100%;
  line-height: 89rpx;
  font-size: 22rpx;
  display: flex;
  justify-content: space-between;
}

